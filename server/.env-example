### Node JS ###

# description: Your Node JS Port
# format: Number
# example: 1337
# required: true
PORT=1337

# description: Your Production URL. You only set this if your planning to go into production.
# format: URL
# example: https://www.test.com
# required: true
PRODUCTION_URL=

### Twilio ####

# description: Your Twilio Account SID
# format: String
# example: ACXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX
# link: https://www.twilio.com/docs/iam/keys/api-key
# required: true
TWILIO_ACCOUNT_SID=

# description: Your Twilio Auth Token
# format: String
# example: ACXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX
# link: http://twilio.com/console
# required: true
TWILIO_AUTH_TOKEN=

# description: Your Twilio API Key
# format: String
# example: SKXXXX
# link: https://www.twilio.com/docs/iam/keys/api-key
# required: true
TWILIO_API_KEY=

# description: Your Twilio API Secret
# format: String
# example: P4SSW0RDXXXXXXXXXXXXXXXXXXXXXXXX
# link: https://www.twilio.com/docs/iam/keys/api-key
# required: true
TWILIO_API_KEY_SECRET=

#
# Temporal.io
#

# description: An address and port number.
# format: URL
# example: <Namespace_ID>.tmprl.cloud
# link: https://docs.temporal.io/application-development/foundations#connect-to-a-cluster
# required: true
TEMPORAL_ADDRESS=

# description: A Namespace is a unit of isolation within the Temporal Platform.
# format: String
# example: <Namespace_ID>
# link: https://docs.temporal.io/application-development/foundations#connect-to-a-cluster
# required: true
TEMPORAL_NAMESPACE=

# description: mTLS CA certificate
# format: File Path
# example: ./tls/***.pem
# link: https://docs.temporal.io/application-development/foundations#connect-to-a-cluster
# required: true
TEMPORAL_TLS_CERT=

# description: mTLS CA private key
# format: File Path
# example: ./tls/***.key
# link: https://docs.temporal.io/application-development/foundations#connect-to-a-cluster
# required: true
TEMPORAL_TLS_KEY=

# description: Temporal Worker's Task Queue
# format: string
# example: helloworld
# link: https://docs.temporal.io/workers#task-queue
# required: true
TEMPORAL_TASK_QUEUE=

# description: The Temporal Server uses the Build ID to determine which versions of a Workflow Definition a Worker can process.
# format: free-form string
# example: ./tls/***.key
# link: https://docs.temporal.io/workers#build-id-reachability
# required: false
TEMPORAL_WORKER_BUILD_ID=

# description: To use local or Temporal Cloud 
# format: Boolean
# example: true
# link: https://legacy-documentation-sdks.temporal.io/typescript/security
# required: true
TEMPORAL_MTLS=

# description: To use a custom Data Converter
# format: Boolean
# example: true
# link: https://docs.temporal.io/security#data-converter
# required: true
TEMPORAL_ENCRYPTION=